@article{Sosnaud2013,
  title = {Class in {{Name Only}}: {{Subjective Class Identity}}, {{Objective Class Position}}, and {{Vote Choice}} in {{American Presidential Elections}}},
  author = {Sosnaud, Benjamin and Brady, David and Frenk, Steven M.},
  year = {2013},
  volume = {60},
  pages = {81--99},
  issn = {00377791},
  doi = {10.1525/sp.2013.10258.This},
  abstract = {Partly because of the widespread tendency for Americans to think of\textbackslash nthemselves as ``middle class,\{''\} subjective class identity often does\textbackslash nnot correspond to objective class position. This study evaluates the\textbackslash nextent to which American voters' subjective class identities differ from\textbackslash ntheir objective class positions. We then evaluate the implications of\textbackslash nsuch differences for voting behavior using American National Election\textbackslash nStudies data from eight recent presidential elections. Coding\textbackslash nrespondents according to whether subjective class identity is higher or\textbackslash nlower than objective class position, we construct a novel schema of\textbackslash ninflated, cleated, and concordant class perceptions. We find that there\textbackslash nare substantial differences between Americans' subjective and objective\textbackslash nsocial class: over two-thirds of the upper-middle class have a deflated\textbackslash nperception of their class position, only half of the middle class have\textbackslash nconcordant perceptions, and more than a third of the working class have\textbackslash ninflated perceptions. We also find that this divergence varies depending\textbackslash non sociodemographic factors, and especially race and education. The\textbackslash nanalyses initially show a pattern that those with inflated class\textbackslash nperceptions are more likely to vote Republican. However, this\textbackslash nrelationship is not significant once we control for race and income.},
  file = {C\:\\Users\\Julio\\Zotero\\storage\\ZL5DJTMF\\Sosnaud, Brady, Frenk - 2013 - Class in Name Only Subjective Class Identity, Objective Class Position, and Vote Choice in American Presi.pdf},
  journal = {Social Problems},
  number = {1}
}

@article{Soule1997,
  title = {The {{Student Divestment Movement}} in the {{United States}} and {{Tactical Diffusion}} : {{The Shantytown Protest}}},
  author = {Soule, Sarah A.},
  year = {1997},
  volume = {75},
  pages = {855--882},
  issn = {0037-7732},
  doi = {10.1093/sf/75.3.855},
  abstract = {A recent trend in the literature on social movements is the focus on how social movement organizations influence not only their challengers but also other social movement organizations, both in other movements and movements in diffrent countries. This article shows how diffusion theory helps us to better understand this process by specifing ways in which social movement organizations within the same movement may influence one another through indirect network ties. More specifically, I show that a new tactic of protest, the shantytown, spread rapidly among U.S. campuses between 1985 and 1990. Recent advances in the modeling of diffusion in an event historyframework allow me to testfor the diffsion of this innovative strategy of protest among certain groupings of colleges and universities. Specifically, my results indicate that the tactic spread among colleges and universities with similar size endowments, of roughly the same level of prestige, and of the same institutional type. My analysis also indicates that high prestige, liberal arts colleges with smaller numbers of Afican American students had higher rates of shantytown protest.},
  file = {C\:\\Users\\Julio\\Zotero\\storage\\VG5L9IDT\\Soule - 1997 - The Student Divestment Movement in the United States and Tactical Diffusion The Shantytown Protest(2).pdf},
  isbn = {0037-7732},
  journal = {Social Forces},
  number = {3}
}

@article{Soule2005,
  title = {A Movement Society Evaluated: {{Collective}} Protest in the {{United States}}, 1960-1986},
  author = {a. Soule, Sarah and Earl, Jennifer},
  year = {2005},
  volume = {10},
  pages = {345--364},
  issn = {1086-671X},
  doi = {10.17813/maiq.10.3.730350353753l022},
  abstract = {In an attempt to make sense of shifts in the social movement sector and its relationship to conventional politics over the past forty years, some have proposed that Western nations are increasingly becoming "movement societies." Accordingly, there are four key characteristics of the movement society: (1) over time expansion of protest; (2) over time diffusion of protest; (3) over time institutionalization of protest; and (4) over time institutionalization of state responses to protest Using newly available data on over 19,000 protest events occur-ring in the US. between 1960 and 1986, we evaluate these four claims. Our findings suggest that movement society scholars are correct in some respects: the size of protest events has grown over time, the percentage of events at which at least one social movement organization is present has increased over time, the number of distinct protest claims has increased over time, and violent forms of protest policing have decreased over time. However, our findings call into question other movement society claims: the number of protests has declined over time, fewer organizations were present at each protest event over time, fewer new groups initiated events over time, fewer new claims emerged over time, and there was more significant activity by groups on the right in the 1960s and 1970s than expected. We suggest potential explanations for some of the negative findings in an attempt to refine the movement society arguments.},
  file = {C\:\\Users\\Julio\\Zotero\\storage\\PH25BVGS\\Soule, Earl - 2005 - A movement society evaluated Collective protest in the United States, 1960-1986(2).pdf},
  isbn = {1086-671X},
  journal = {Mobilization},
  keywords = {organizations,professionalization},
  number = {3}
}

