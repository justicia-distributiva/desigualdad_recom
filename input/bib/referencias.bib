@incollection{leander2004wars,
  title = {Wars and the Un-Making of States: Taking {{Tilly}} Seriously in the Contemporary World},
  booktitle = {Contemporary Security Analysis and {{Copenhagen}} Peace Research},
  author = {Leander, Anna},
  year = {2004},
  pages = {85--96},
  publisher = {{Routledge}},
  issn = {0031-3599},
  doi = {10.4324/9780203356913},
  abstract = {The only trouble with all of this is that, in spite of its respectable origins, its realism and its usefulness, the `war makes states' argument no longer holds. The main reason for this is that contemporary state build- ing takes place in a globalised context which alters the effects of the central processesx which Tilly (and others) argued placed war-making and state-making in a positive relationship. The thrust of the argument is that these mechanisms and processes continue to be useful and essential for understanding the relationship between wars and states. However, if we look at how they work, it soon becomes clear that they fail to create a positive link between wars and states. The point is therefore not to contest Tilly's analysis of European situation. Rather, the chapter shows that if we take Tilly seriously \textendash{} i.e. follow the arguments he actually devel- ops and grant his argument more than folklore status \textendash{} it helps us to understand why wars at present do not make states, but rather unravel them.},
  file = {C\:\\Users\\Julio\\Zotero\\storage\\H2S2Q9P5\\Leander - 2004 - Wars and the un-making of states taking Tilly seriously in the contemporary world.pdf},
