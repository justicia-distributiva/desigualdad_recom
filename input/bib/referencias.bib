@article{Dhami2010,
  title = {Redistributive Policies with Heterogeneous Social Preferences of Voters},
  author = {Dhami, S. and {al-Nowaihi}, Ali},
  year = {2010},
  volume = {54},
  pages = {743--759},
  publisher = {{Elsevier}},
  issn = {00142921},
  doi = {10.1016/j.euroecorev.2010.01.001},
  abstract = {There is growing evidence on the roles of fairness and other-regarding preferences as fundamental human motives. Call voters with fair preferences, as in Fehr and Schmidt (1999), fair-voters. By contrast, traditional political economy models are based on selfish-voters who derive utility solely from "own" payoff. In a general equilibrium model with endogenous labor supply, a mixture of fair and selfish voters choose optimal policy through majority voting. First, we show that majority voting produces a unique winner in pairwise contests over feasible policies (the Condorcet winner). Second, we show that a preference for greater fairness leads to greater redistribution. An increase in the number of fair voters can also lead to greater redistribution. Third, we show that in economies where the majority are selfish-voters, the decisive policy could be chosen by fair-voters, and vice versa. Fourth, while choosing labor supply, even fair voters behave exactly like selfish voters. We show how this apparently inconsistent behavior in different domains (voting and labor supply) can be rationalized within the model. \textcopyright{} 2010 Elsevier B.V.},
  isbn = {0014-2921},
  journal = {European Economic Review},
  keywords = {Direct democracy,Other-regarding preferences,redistribution,Redistribution},
  number = {6}
}

