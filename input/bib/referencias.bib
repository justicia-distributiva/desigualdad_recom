@article{Smith2012,
  title = {Two {{Decades}} of {{Value Change}}: {{The Crystallization}} of {{Meritocratic}} and {{Egalitarian Beliefs}} in the {{Czech Republic}}},
  author = {Smith, Michael L. and Mat{\v e}j{\r{u}}, Petr},
